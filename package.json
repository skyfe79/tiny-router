{
  "name": "tiny-router",
  "version": "1.0.0",
  "description": "A tiny router supporting Express-style route patterns",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "vite build",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "router",
    "tiny",
    "express",
    "routing",
    "path-matching",
    "url-pattern"
  ],
  "author": "skyfe79",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/skyfe79/tiny-router.git"
  },
  "bugs": {
    "url": "https://github.com/skyfe79/tiny-router/issues"
  },
  "homepage": "https://github.com/skyfe79/tiny-router#readme",
  "devDependencies": {
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  }
}
